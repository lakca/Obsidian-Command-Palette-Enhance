{
	"name": "command-palette-enhancer",
	"version": "1.0.1",
	"description": "Enhance command palette for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"command",
		"history",
		"search"
	],
	"author": "lakca",
	"license": "MIT",
	"release": {
		"repositoryUrl": "git@github.com:lakca/obsidian-command-palette-enhancer.git",
		"branches": [
			"master",
			"release/*"
		],
		"tagFormat": "${version}",
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			[
				"@semantic-release/exec",
				{
					"prepareCmd": "npm_package_version=${nextRelease.version} node version-bump.mjs"
				}
			],
			[
				"@semantic-release/git",
				{
					"assets": [
						"CHANGELOG.md",
						"manifest.json"
					]
				}
			],
			[
				"@semantic-release/github",
				{
					"assets": [
						{
							"path": "main.js"
						},
						{
							"path": "manifest.json"
						},
						{
							"path": "styles.css"
						}
					]
				}
			]
		]
	},
	"files": [
		"main.js",
		"package.json",
		"manifest.json",
		"styles.css"
	],
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"builtin-modules": "^3.2.0",
		"esbuild": "0.13.12",
		"eslint": "^8.9.0",
		"obsidian": "^0.12.17",
		"semantic-release": "^24.1.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"esbuild-plugin-import-glob": "^0.1.1"
	},
	"volta": {
		"node": "18.20.4"
	},
	"packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf"
}
